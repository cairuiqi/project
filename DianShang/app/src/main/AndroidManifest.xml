<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bwei.crq">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="design_width_in_dp"
            android:value="360" />
        <meta-data
            android:name="design_height_in_dp"
            android:value="640" />

        <activity android:name=".acitvity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".acitvity.LoginActivity" />
        <activity android:name=".acitvity.RegActivity" />
        <activity android:name=".acitvity.DetailsActivity" />
        <activity android:name=".acitvity.shopingactivity.PayActivity" />
        <activity android:name=".acitvity.myactivity.InfoActivity" />
        <activity android:name=".acitvity.myactivity.CircleActivity" />
        <activity android:name=".acitvity.myactivity.FootActivity" />
        <activity android:name=".acitvity.myactivity.AddadresActivity" />
        <activity android:name=".acitvity.myactivity.AddadressNextActivity" />
        <activity android:name=".acitvity.myactivity.WalletActivity" />
        <!-- 订创建单 -->
        <activity android:name=".acitvity.orderactivity.OrderActivity" />
        <activity android:name=".acitvity.ThreeListActivity"></activity>
    </application>

</manifest>